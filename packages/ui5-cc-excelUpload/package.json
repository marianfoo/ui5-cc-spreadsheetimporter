{
  "name": "ui5-cc-excelupload",
  "version": "0.16.1",
  "description": "A UI5 Module to integrate a Excel Upload for Fiori Element Apps.",
  "main": "Component.js",
  "keywords": [
    "ui5",
    "custom control",
    "xlsx",
    "Excel"
  ],
  "author": "Marian Zeis",
  "homepage": "https://marianfoo.github.io/ui5-cc-excelUpload/",
  "repository": {
    "type": "git",
    "url": "https://github.com/marianfoo/ui5-cc-excelUpload.git",
    "directory": "packages/ui5-cc-excelUpload"
  },
  "files": [
    "dist",
    "ui5.yaml"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm-run-all build --parallel start:server start:v4fe:108",
    "start:v2fe:108": "pnpm --filter ordersv2fe108 start",
    "start:v4fe:108": "pnpm --filter ordersv4fe108 start",
    "test:v4fe:108": "pnpm --filter ui5-cc-excelupload-sample test -- -- ordersv4fe 108",
    "test:v2fe:108": "pnpm --filter ui5-cc-excelupload-sample test -- -- ordersv2fe 108",
    "start:server": "pnpm --filter ui5-cc-excelupload-sample watch:server",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint -e",
    "prettier": "prettier --write .",
    "prettier:staged": "pretty-quick --staged --verbose",
    "hooks:pre-commit": "npm-run-all --sequential prettier:staged lint:staged",
    "hooks:pre-push": "npm-run-all --sequential lint:commit",
    "build": "ui5 build --config=ui5-build.yaml --all --clean-dest --dest dist",
    "build:dev": "ui5 build --config=ui5-build.yaml --all --clean-dest --dest dist  --exclude-task=replaceCopyright replaceVersion generateFlexChangesBundle generateComponentPreload generateVersionInfo minify escapeNonAsciiCharacters",
    "replaceString": "node ./dev/replace-string.js",
    "build:watch": "node ./dev/chokidar.js",
    "updateChromedriver": "cd examples && pnpm add chromedriver@latest --save-dev -w",
    "copyTestApps": "node ./dev/copy-example-apps.js create && pnpm i --no-frozen-lockfile",
    "deleteAllNodeModules": "node dev/util.js deleteNodeModules .",
    "deleteTestApps": "node ./dev/copy-example-apps.js  delete",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml -y"
  },
  "dependencies": {
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@prettier/plugin-xml": "^2.2.0",
    "@sap/ux-ui5-tooling": "1.9.5",
    "@sapui5/ts-types-esm": "1.111.4",
    "@types/node": "20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@ui5/cli": "3.1.2",
    "@ui5/ts-interface-generator": "0.5.5",
    "babel-preset-transform-ui5": "^7.1.4",
    "bluebird": "3.7.2",
    "chokidar": "^3.5.3",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-properties": "^0.2.0",
    "pretty-quick": "^3.1.3",
    "replace-in-file": "^6.3.5",
    "shelljs": "^0.8.5",
    "typescript": "5.0.4",
    "ui5-tooling-modules": "0.9.8",
    "ui5-tooling-stringreplace": "0.1.3",
    "ui5-tooling-transpile": "0.7.8"
  }
}
