<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Dialog title="{i18n>exceluploadPlaceholder}" resizable="true" state="Error" verticalScrolling="false" contentHeight="50%" contentWidth="50%">
    <content>
      <MessageView items="{errorData>/}">
        <MessageItem title="{errorData>title}" type="Error" counter="{errorData>counter}" subtitle="{errorData>subtitle}" description="{errorData>description}" />
      </MessageView>
    </content>
    <endButton>
      <Button press=".onCloseErrorDialog" text="Ok" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>
