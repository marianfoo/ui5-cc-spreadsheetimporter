{
  "name": "ui5-cc-spreadsheetimporter",
  "version": "0.25.3",
  "description": "A UI5 Module to integrate a Spreadsheet Upload for Fiori Element Apps.",
  "main": "dist/Component.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ui5",
    "custom control",
    "xlsx",
    "Excel"
  ],
  "author": "Marian Zeis",
  "homepage": "https://spreadsheet-importer.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/marianfoo/ui5-cc-spreadsheetimporter.git",
    "directory": "packages/ui5-cc-spreadsheetimporter"
  },
  "files": [
    "dist",
    "ui5.yaml"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm-run-all build --parallel start:server start:v4fe:108",
    "start:v2fe:108": "pnpm --filter ordersv2fe108 start",
    "start:v4fe:108": "pnpm --filter ordersv4fe108 start",
    "test:v4fe:108": "pnpm --filter ui5-cc-spreadsheetimporter-sample test -- -- ordersv4fe 108",
    "test:v2fe:108": "pnpm --filter ui5-cc-spreadsheetimporter-sample test -- -- ordersv2fe 108",
    "start:server": "pnpm --filter ui5-cc-spreadsheetimporter-sample watch:server",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint -e",
    "prettier": "prettier --write .",
    "prettier:staged": "pretty-quick --staged --verbose",
    "hooks:pre-commit": "npm-run-all --sequential prettier:staged lint:staged",
    "hooks:pre-push": "npm-run-all --sequential lint:commit",
    "build": "ui5 build --config=ui5-build.yaml --all --clean-dest --dest dist",
    "build:dev": "ui5 build --config=ui5-build-dev.yaml --all --dest dist  --exclude-task=replaceCopyright replaceVersion generateFlexChangesBundle generateComponentPreload generateVersionInfo minify escapeNonAsciiCharacters",
    "build:dev:tooling": "ui5 build --config=ui5-build.yaml --all --dest dist  --exclude-task=replaceCopyright replaceVersion generateFlexChangesBundle generateComponentPreload generateVersionInfo minify escapeNonAsciiCharacters",
    "replaceString": "node ./dev/replace-string.js",
    "build:watch": "node ./dev/chokidar.js",
    "updateChromedriver": "cd examples && pnpm add chromedriver@latest --save-dev -w",
    "copyTestApps": "node ./dev/copy-example-apps.js create && pnpm i --no-frozen-lockfile",
    "deleteAllNodeModules": "node dev/util.js deleteNodeModules .",
    "deleteTestApps": "node ./dev/copy-example-apps.js  delete",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml -y"
  },
  "dependencies": {
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@prettier/plugin-xml": "^3.2.1",
    "@sap/ux-ui5-tooling": "1.11.0",
    "@sapui5/ts-types-esm": "1.118.0",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@ui5/cli": "3.6.0",
    "@ui5/ts-interface-generator": "0.8.1",
    "babel-preset-transform-ui5": "^7.2.5",
    "bluebird": "3.7.2",
    "chokidar": "^3.5.3",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-properties": "^0.2.0",
    "pretty-quick": "^3.1.3",
    "replace-in-file": "^7.0.1",
    "shelljs": "^0.8.5",
    "typescript": "5.2.2",
    "ui5-tooling-modules": "3.0.7",
    "ui5-tooling-stringreplace": "3.0.1",
    "ui5-tooling-transpile": "3.2.3"
  }
}
