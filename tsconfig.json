{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./packages/",
    "baseUrl": "./",
    "typeRoots": ["node_modules/@types", "node_modules/@sapui5/ts-types-esm"],
    "paths": {
      "cc/excelUpload/XXXnamespaceXXX/*": ["./packages/ui5-cc-excelUpload/src/*"],
      "cc/excelUploadButton/XXXnamespaceXXX/*": ["./packages/ui5-cc-excelUpload-Button/*"]
    }
  },
  "include": ["./packages/**/*"]
}
