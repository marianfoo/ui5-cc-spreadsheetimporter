{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "declaration": true,
    "rootDir": "./packages/",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "./node_modules/@sapui5/types"],
    "paths": {
      "cc/spreadsheetimporter/XXXnamespaceXXX/*": ["packages/ui5-cc-spreadsheetimporter/src/*"]
    }
  },
  "include": ["packages/ui5-cc-spreadsheetimporter/**/*"],
  "exclude": ["node_modules", "dist", "packages/ui5-cc-spreadsheetimporter/dist"]
}
