{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "inlineSourceMap": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "rootDir": "webapp",
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "ui/v4/ordersv4fets/*": [
        "webapp/*"
      ]
    },
    "types": [ "@sapui5/ts-types-esm", "ui5-cc-spreadsheetimporter" ],
    "typeRoots": ["./node_modules"]
  },
  "include": [
    "webapp/**/*"
  ]
}