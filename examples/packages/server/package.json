{
  "name": "@capire/orders",
  "version": "1.0.0",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "1.13.0",
    "@sap/cds": "^8.0.4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.7.3",
    "cds-launchpad-plugin": "2.2.0",
    "cds-plugin-ui5": "0.9.3",
    "ordersv2fe120": "workspace:*",
    "ordersv4fe120": "workspace:^"
  },
  "scripts": {
    "watch": "cds watch"
  },
  "cds": {
    "fiori": {
      "lean_draft": true,
      "bypass_draft": true
    },
    "cds-plugin-ui5": {
      "modules": {
        "ordersv4fe120": {
          "configFile": "ui5.yaml"
        },
        "ordersv2fe120": {
          "configFile": "ui5.yaml"
        }
      }
    },
    "launchpad": {
      "theme": "sap_horizon",
      "version": "1.120.6",
      "basePath": "launchpad.html",
      "appConfigPath": "app/appconfig.json"
    }
  }
}
